<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">



	<navigation-rule>
		<from-view-id>/paginas/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/paginas/page_main.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
	<from-view-id>/paginas/templatePrincipal.xhtml</from-view-id>
	<navigation-case>
		<from-outcome>employeRegister</from-outcome>
		<to-view-id>/paginas/employe_register.xhtml</to-view-id>
		</navigation-case>
	
	</navigation-rule>
	<application>
		<!-- Mensagens do sistama -->
		<message-bundle>br.com.spbweb.resource.Messages</message-bundle>
		<!-- Internacionalização -->
		<locale-config>
			<default-locale>pt_BR</default-locale>
			<supported-locale>pt_BR</supported-locale>
		</locale-config>
	</application>

</faces-config>
